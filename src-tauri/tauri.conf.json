{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "LaburAR",
	"version": "1.0.5",
	"identifier": "com.laburar.app",
	"build": {
		"beforeDevCommand": "yarn dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "yarn build",
		"frontendDist": "../build"
	},
	"app": {
		"windows": [
			{
				"title": "LaburAR",
				"center": true,
				"minWidth": 800,
				"minHeight": 600,
				"width": 1366,
				"height": 920,
				"label": "LaburAR",
				"maximized": true
			}
		],
		"security": {
			"csp": null
		}
	},
	"plugins": {
		"updater": {
			"windows": {
				"installMode": "passive"
			},
			"pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEVDRjlCOEVBRURENDAwMkEKUldRcUFOVHQ2cmo1N0tpSGFNQXhwVWRDSGc1VEJ1MXJ5SmxhYU9HdVVld1Q4dWt1YkFZYW5saVQK",
			"endpoints": ["https://github.com/facundon/laburar/releases/latest/download/latest.json"]
		}
	},
	"bundle": {
		"active": true,
		"createUpdaterArtifacts": true,
		"targets": "nsis",
		"publisher": "Facundon",
		"windows": {
			"nsis": {
				"installMode": "perMachine"
			}
		},
		"icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
	}
}
